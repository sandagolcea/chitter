
  <ul>
  <% @peeps.each do |peep| %>
    <li><%= peep.message %></li>
  <% end %>
  </ul>

  <div id="new-peep">
  <form action="/peeps" method="post">
    Message: <input type="text" name="message">
    Hashtags: <input type="text" name="hashtags">
    <input type="submit" value="Add peep">
  </form>
</div>

